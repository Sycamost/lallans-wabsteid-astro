---
import type { Contributor } from '$types/LallansIssue';

interface Props {
  contributors: Contributor[];
}

const { contributors } = Astro.props;
---

<section>
  <h2>Contributors</h2>
  <ul class="product__contributors">
    {contributors.map((contributor) => <li>{contributor}</li>)}
  </ul>
</section>
