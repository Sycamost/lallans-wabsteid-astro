---
import Footer from '$components/Footer.astro';
import Navbar from '$components/Navbar/Navbar.astro';
import Layout from './Layout.astro';

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<Layout title={title}>
  <Navbar />
  <main id="main">
    <slot />
  </main>
  <Footer />
</Layout>
